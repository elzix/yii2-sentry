{
  "name": "elzix/yii2-sentry",
  "description": "Yii2 integration for Sentry (SDK v3)",
  "keywords": [
    "logging",
    "errors",
    "yii2",
    "sentry",
    "v3",
    "sdk"
  ],
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Erix Kivuti",
      "email": "info@contemplate.me.ke"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^8.1",
    "sentry/sentry": "^3.18",
    "yiisoft/yii2": "~2.0.45"
  },
  "require-dev": {
    "php-http/mock-client": "^1.0"
  },
  "suggest": {
    "php-http/curl-client": "Recommended HTTP client for Sentry SDK",
    "http-interop/http-factory-guzzle": "Recommended HTTP factory for Sentry SDK"
  },
  "autoload": {
    "psr-4": {
      "elzix\\Yii2\\Sentry\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
